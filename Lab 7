Exercício 1
MOV TL0, #16
CPL P1.0
org 001Bh
INT_TEMP1:
MOV TH1, #136
MOV TL1, #136
CPL P1.4
RETI
org 0080h
START:
MOV TMOD, #22h
MOV TH0, #16
MOV TL0, #16
MOV TH1, #136
MOV TL1, #136
SETB EA
SETB ET0
SETB ET1
SETB TR0
SETB TR1
SJMP $

Exercício 2
org 0000h
LJMP START
org 0003h
INT_TEMP0:
INC P1
RETI
org 0013h
INT_TEMP1:
DEC P1
RETI
org 0080h
START:
SETB EA
SETB EX0
SETB EX1
SJMP $
